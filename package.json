{
  "private": true,
  "name": "isabelle",
  "version": "1.12.4",
  "description": "Isabelle est un BOT Discord à destination des élèves de TELECOM Nancy, et plus particulièrement aux étudiants de la promotion 2027 des FISA",
  "author": "MAXOUXAX",
  "engines": {
    "node": ">=22"
  },
  "packageManager": "npm@11.10.0",
  "license": "AGPL-3.0-or-later",
  "homepage": "https://github.com/MAXOUXAX/Isabelle",
  "contributors": [
    "MAXOUXAX <24844231+MAXOUXAX@users.noreply.github.com>",
    "Tanguy <tanguyfox@gmail.com>",
    "Ozraam <55456920+Ozraam@users.noreply.github.com>"
  ],
  "bugs": {
    "url": "https://github.com/MAXOUXAX/Isabelle/issues"
  },
  "maintainers": [
    {
      "name": "MAXOUXAX",
      "email": "24844231+MAXOUXAX@users.noreply.github.com",
      "url": "https://github.com/MAXOUXAX"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MAXOUXAX/Isabelle.git"
  },
  "exports": "./src/index.js",
  "type": "module",
  "scripts": {
    "start": "drizzle-kit migrate && node index.mjs",
    "start:dev": "tsx watch src/index.ts",
    "dev": "cross-env NODE_ENV=development npm-run-all --serial db:migrate start:dev",
    "build": "npm-run-all --serial build:ts build:copy-files",
    "build:ts": "tsup",
    "build:copy-files": "cp -r drizzle dist/ && cp drizzle.config.ts dist/",
    "lint": "eslint . --fix",
    "lint:dry": "eslint .",
    "prettier": "prettier --ignore-path .gitignore --write \"./src/**/*.+(js|ts|json)\"",
    "prettier:dry": "prettier --ignore-path .gitignore --check \"./src/**/*.+(js|ts|json)\"",
    "prepare": "husky",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "sutom:generate-wordlist": "tsx bin/generate-wordlist.ts"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier -w"
    ]
  },
  "dependencies": {
    "@ai-sdk/google": "^3.0.29",
    "@libsql/client": "^0.17.0",
    "@napi-rs/canvas": "^0.1.93",
    "@resvg/resvg-js": "^2.6.2",
    "ai": "^6.0.86",
    "discord.js": "^14.25.1",
    "dotenv": "^17.3.1",
    "drizzle-orm": "^0.45.1",
    "node-ical": "^0.25.2",
    "pino": "^10.3.1",
    "pino-pretty": "^13.1.3",
    "react": "^19.2.4",
    "satori": "^0.19.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.6",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "cross-env": "^10.1.0",
    "drizzle-kit": "^0.31.9",
    "emittery": "^1.2.0",
    "eslint": "^10.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.8.1",
    "semantic-release": "^25.0.3",
    "semantic-release-discord": "^1.2.0",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0"
  }
}
