{
  "private": true,
  "name": "isabelle",
  "version": "1.6.3",
  "description": "Isabelle est un BOT Discord à destination des élèves de TELECOM Nancy, et plus particulièrement aux étudiants de la promotion 2027 des FISA",
  "author": "MAXOUXAX",
  "engines": {
    "node": ">=22"
  },
  "packageManager": "npm@10.9.0",
  "license": "AGPL-3.0-or-later",
  "homepage": "https://github.com/MAXOUXAX/Isabelle",
  "contributors": [
    "MAXOUXAX <24844231+MAXOUXAX@users.noreply.github.com>",
    "Tanguy <tanguyfox@gmail.com>",
    "Ozraam <55456920+Ozraam@users.noreply.github.com>"
  ],
  "bugs": {
    "url": "https://github.com/MAXOUXAX/Isabelle/issues"
  },
  "maintainers": [
    {
      "name": "MAXOUXAX",
      "email": "24844231+MAXOUXAX@users.noreply.github.com",
      "url": "https://github.com/MAXOUXAX"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MAXOUXAX/Isabelle.git"
  },
  "exports": "./src/index.js",
  "type": "module",
  "scripts": {
    "start": "drizzle-kit migrate && node index.mjs",
    "start:dev": "tsx watch src/index.ts",
    "dev": "cross-env NODE_ENV=development npm-run-all --serial db:migrate start:dev",
    "build": "npm-run-all --serial build:ts build:copy-files",
    "build:ts": "tsup",
    "build:copy-files": "cp -r drizzle dist/ && cp drizzle.config.ts dist/",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --ignore-path .gitignore --write \"./src/**/*.+(js|ts|json)\"",
    "prettier:fix": "prettier --write src",
    "prepare": "husky",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier -w"
    ]
  },
  "dependencies": {
    "@libsql/client": "^0.15.15",
    "@napi-rs/canvas": "^0.1.80",
    "discord.js": "^14.18.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.5",
    "node-ical": "^0.21.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@types/eslint__js": "^9.14.0",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "cross-env": "^10.0.0",
    "drizzle-kit": "^0.30.5",
    "eslint": "^9.21.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "semantic-release": "^24.2.9",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.0"
  }
}
