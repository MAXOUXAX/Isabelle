{
  "private": true,
  "name": "isabelle",
  "version": "1.0.1",
  "description": "Isabelle est un BOT Discord à destination des élèves de TELECOM Nancy, et plus particulièrement aux étudiants de la promotion 2027 des FISA",
  "author": "MAXOUXAX",
  "engines": {
    "node": ">=22"
  },
  "packageManager": "npm@10.9.0",
  "license": "AGPL-3.0-or-later",
  "homepage": "https://github.com/MAXOUXAX/Isabelle",
  "contributors": [
    "MAXOUXAX <24844231+MAXOUXAX@users.noreply.github.com>"
  ],
  "bugs": {
    "url": "https://github.com/MAXOUXAX/Isabelle/issues"
  },
  "maintainers": [
    {
      "name": "MAXOUXAX",
      "email": "24844231+MAXOUXAX@users.noreply.github.com",
      "url": "https://github.com/MAXOUXAX"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MAXOUXAX/Isabelle.git"
  },
  "exports": "./src/index.js",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx watch src/index.ts",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "build": "tsup src/index.ts --minify --sourcemap --format esm --tsconfig tsconfig.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --ignore-path .gitignore --write \"./src/**/*.+(js|ts|json)\"",
    "prettier:fix": "prettier --write src",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier -w"
    ]
  },
  "dependencies": {
    "discord.js": "^14.17.2",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@types/eslint__js": "^8.42.3",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.16.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "prettier": "^3.4.2",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.17.0"
  }
}
